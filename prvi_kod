a=[10,20,24,25,17,16]# suma svih neparnih brojeva
suma=0
for i in a:
    if i%2==0:
        suma+=i
print(suma)
#uensi dva broja i ispisi njihov zbroj, razliku umnozak i količnik
b=int(input("prvi broj :"))
c=int(input("drugi broj :"))
print(b+c)
print(b-c)
print(b*c)
if(c!=0):
    print(b/c)
else:
    print("C je 0")
#unesi broj n i ispisi sumu svih brojeva od 1 do n
n=int(input("unesi n"))
i=0
zbroj=0
while i<=n:
    zbroj+=i
    i+=1
print(zbroj)

#unesi broj n i ispisi sumu svih neparnih brojeva od 1 do n
#koristim isti n od gore
i=0
zbroj=0
while i<=n:
    if(i%2==0):
        zbroj+=i
    i+=1  #ovo je iza zato što u svakoj iteraciji se i mora ažurirati inaće će biti beskonačna petlja
print(zbroj)


